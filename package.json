{
    "name": "virtual-merge",
    "version": "1.0.2",
    "description": "A utility library to merge multiple Objects virtually",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc && esbuild --define:module=undefined --format=esm src/index.ts --outfile=dist/index.mjs",
        "test": "tsx src/test.ts",
        "prepublishOnly": "pnpm build"
    },
    "keywords": [
        "merge",
        "objects"
    ],
    "author": "Vendicated <vendicated+npm@riseup.net>",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^18.15.0",
        "typescript": "^5.7.3"
    },
    "exports": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
    },
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/Vendicated/virtual-merge.git"
    },
    "homepage": "https://github.com/Vendicated/virtual-merge"
}
